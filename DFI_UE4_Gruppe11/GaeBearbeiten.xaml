<Window x:Class="DFI_UE4_Gruppe11.GaeBearbeiten"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DFI_UE4_Gruppe11"
        mc:Ignorable="d"
        Title="Gärtner bearbeiten" Height="210" Width="350">
    <Grid>
        <Label Content="Nachname:" HorizontalAlignment="Left" Margin="29,24,0,0" VerticalAlignment="Top" Width="88"/>
        <Label Content="Vorname:" HorizontalAlignment="Left" Margin="29,74,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.068,-1.664" Width="88"/>
        <TextBox x:Name="NachnameTextbox" HorizontalAlignment="Left" Height="21" Margin="136,29,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="198"/>
        <TextBox x:Name="VornahmeTextbox" HorizontalAlignment="Left" Height="23" Margin="136,74,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="198"/>
        <CheckBox x:Name="StandCheckbox" Content="Aktiv" HorizontalAlignment="Left" Margin="136,114,0,0" VerticalAlignment="Top" IsChecked="True"/>
        <Button x:Name="GaeOkButton" Content="OK" HorizontalAlignment="Left" Margin="44,149,0,0" VerticalAlignment="Top" Width="256" Click="GaeOkButton_Click"/>

    </Grid>
</Window>
